   "source": [
    "n = int(input(\"Enter a positive integer: \").strip())\n",
    "sum_even = 0\n",
    "for i in range(2, n + 1, 2):\n",
    "    sum_even += i\n",
    "\n",
    "print(f\"The sum of all even numbers between 1 and {n} is: {sum_even}\")"
   ]
